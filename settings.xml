<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
          xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    
    <profiles>
        <profile>
            <id>it4ng.internal</id>
            <properties>
                <altDeploymentRepository>it4ng.internal::default::${env.DEPLOYMENT_REPOSITORY}</altDeploymentRepository>
                <altSnapshotDeploymentRepository>it4ng.internal::default::${env.DEPLOYMENT_REPOSITORY}</altSnapshotDeploymentRepository>
                <altReleaseDeploymentRepository>it4ng.internal::default::${env.DEPLOYMENT_REPOSITORY}</altReleaseDeploymentRepository>
            </properties>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>it4ng.internal</activeProfile>
    </activeProfiles>
          
    <servers>
        <server>
            <id>it4ng.internal</id>
            <username>${env.MAVEN_REPO_USER}</username>
            <password>${env.MAVEN_REPO_PASS}</password>
        </server>
    </servers>
</settings>
